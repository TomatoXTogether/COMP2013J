<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test.Login">
   <children>
      <Text layoutX="385.0" layoutY="365.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Account" />
      <Text layoutX="385.0" layoutY="420.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      <TextField fx:id="Account" accessibleText="int" layoutX="453.0" layoutY="348.0" onAction="#enterAccount" />
      <TextField fx:id="Password" layoutX="453.0" layoutY="405.0" onAction="#enterPassword" />
      <Text layoutX="385.0" layoutY="310.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Identity" />
      <ChoiceBox fx:id="choseIdentity" layoutX="453.0" layoutY="294.0" onDragDetected="#choseIdentity" prefWidth="150.0" />
      <Button fx:id="login" layoutX="550.0" layoutY="467.0" mnemonicParsing="false" onAction="#Login" text="Login" />
      <Button fx:id="reset" layoutX="385.0" layoutY="467.0" mnemonicParsing="false" onAction="#Reset" text="Reset" />
   </children>
</AnchorPane>
